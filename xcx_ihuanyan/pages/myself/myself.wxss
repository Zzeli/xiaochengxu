/* myself.wxss */
.info{
  width: 100%;
  padding: 58rpx 0;
   background-image: linear-gradient(146deg, #467bf2, #53c2f6);
   display: flex;
   flex-direction: column;
    align-items:center;
}
.portrait{
  width: 142rpx;
  height: 142rpx;
  background: #fff;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 20rpx;
}
.portrait image{
  width: 131rpx;
  height: 131rpx;
  background: pink;
   border-radius: 50%;
}
.info text{
   font-family: "微软雅黑";
  font-size: 24rpx;
  font-weight: 500;
  text-align: center;
  color: #fdf5f5;
}
.weui-cell{
  padding: 25rpx 34rpx;
}
.weui-cell__hd{
  margin-right: 15rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.weui-cell_primary{
   font-family: "微软雅黑";
  font-size: 24rpx;
  font-weight: 500;
  color: #37383b;
}
navigator:nth-child(1) image{
      width: 23rpx!important;
      height: 25rpx!important;
}
navigator:nth-child(2) image{
      width: 25rpx!important;
      height: 23rpx!important;
}
navigator:nth-child(3) image{
      width: 27rpx!important;
      height: 23rpx!important;
}
navigator:nth-child(4) image{
      width: 25rpx!important;
      height: 25rpx!important;
}
navigator:nth-child(5) image{
      width: 23rpx!important;
      height: 29rpx!important;
}
.weui-cell_active{
  background: #f3f1f1;
}