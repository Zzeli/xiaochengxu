.containers {
  box-sizing: border-box;
  padding: 0 22rpx;
}

/* audio.wxss */

.audio {
  width: 100%;
  height: 100vh;
  background: #fff no-repeat left -10%/100% 460rpx;
}

.nav {
  width: 100%;
  height: 36rpx;
  display: flex;
  justify-content: space-around;
}

.nav .nav-list {
  width: auto;
  height: auto;
  font-size: 24rpx;
  color: #fff;
  position: relative;
}

.nav .nav-list:first-child {
  padding-left: 0.32rpx;
}

.nav .active {
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  top: 0;
  left: -32rpx;
  background: no-repeat center/cover;
  background-image: url("data:image/png;base64,");
}

.search {
  width: 100%;
  height: 102rpx;
  display: flex;
  align-items: center;
}

.search input {
  width: 94vw;
  color: #c1c2ce;
  min-height: 69rpx;
  background: #fff;
  font-size: 24rpx;
  border-radius: 10rpx;
  text-align: center;
}

.main-content {
  width: 100%;
  height: auto;
  padding-top: 10rpx;
  display: flex;
  flex-flow: wrap row;
}

.main-content .main-item {
  box-sizing: border-box;
  border: 1px solid #fff;
  background: no-repeat center/cover;
}

.main-content .main-item:nth-child(1) {
  width: 100%;
  height: 396rpx;
}

.main-content .main-item:nth-child(2), .main-content .main-item:nth-child(5) {
  width: 292rpx;
  height: 212rpx;
  margin-top: -1px;
}

.main-content .main-item:nth-child(3), .main-content .main-item:nth-child(4) {
  width: 210rpx;
  height: 212rpx;
  margin-top: -1px;
  margin-left: -1px;
}

.main-content .main-item:nth-child(5) {
  height: 333rpx;
}

.main-content .main-item:nth-child(6) {
  height: 333rpx;
  width: 414rpx;
}

.rink-nav {
  width: 100%;
  height: 60rpx;
  box-sizing: border-box;
  border: 1px solid #fff;
  border-radius: 0.2rem;
  display: flex;
  overflow: hidden;
  color: #222;
  font-size: 24rpx;
  margin: 0 0 0.22rpx 0;
}

.rink-nav .rink-nav-item {
  background: #fff;
  height: 100%;
  flex: 1;
  text-align: center;
  line-height: 60rpx;
}

.rink-nav .rink-nav-item.active {
  color: #fff;
  background: transparent;
}

.singerlist {
  width: 100%;
  height: auto;
  display: flex;
  flex-flow: row wrap;
  padding-top: 10rpx;
  padding-right: 26rpx;
  overflow: hidden;
}

.singerlist .singerList-main {
  width: 30%;
  height: 353rpx;
  margin-right: 20rpx;
}

.singerlist .img {
  height: 300rpx;
  width: 100%;
  background: #fff;
  box-sizing: border-box;
  border: 1px solid #fff;
  border-radius: 8rpx;
  box-shadow: 0 0 10rpx rgba(253, 49, 145, 0.19);
  overflow: hidden;
}

.singerlist .img-con {
  width: 100%;
  height: 100%;
}

.singerlist .title {
  width: 100%;
  height: 53rpx;
  display: flex;
  align-items: center;
  font-size: 24rpx;
  color: #575861;
}
